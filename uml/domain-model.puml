@startuml domain model

class FamilyMember {
    name: string
    gender: string
}

class Mother {
    child: Child[]
    spouse: Father
}

class Father {
    child: Child[]
    spouse: Mother
}

class Child {
    father: Father
    mother: Mother
}

FamilyMember <|-- Mother
FamilyMember <|-- Father
FamilyMember <|-- Child

Mother "1" -- "*" Child
Father "1" -- "*" Child
Mother -- Father

@enduml